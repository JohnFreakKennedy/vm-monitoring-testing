{
    "annotations": {
      "list": []
    },
    "description": "Comprehensive dashboard for monitoring the spam2000 application",
    "editable": true,
    "graphTooltip": 0,
    "panels": [
      {
        "type": "graph",
        "title": "Requests Per Second",
        "datasource": "VictoriaMetrics",
        "targets": [
          {
            "expr": "rate(spam2000_requests_total[1m])",
            "legendFormat": "Requests",
            "refId": "A"
          }
        ],
        "id": 1,
        "gridPos": {
          "h": 6,
          "w": 8,
          "x": 0,
          "y": 0
        }
      },
      {
        "type": "graph",
        "title": "Error Rate",
        "datasource": "VictoriaMetrics",
        "targets": [
          {
            "expr": "rate(spam2000_errors_total[1m])",
            "legendFormat": "Errors",
            "refId": "B"
          }
        ],
        "id": 2,
        "gridPos": {
          "h": 6,
          "w": 8,
          "x": 8,
          "y": 0
        }
      },
      {
        "type": "graph",
        "title": "Success Rate",
        "datasource": "VictoriaMetrics",
        "targets": [
          {
            "expr": "rate(spam2000_success_total[1m])",
            "legendFormat": "Successes",
            "refId": "C"
          }
        ],
        "id": 3,
        "gridPos": {
          "h": 6,
          "w": 8,
          "x": 16,
          "y": 0
        }
      },
      {
        "type": "graph",
        "title": "95th Percentile Latency",
        "datasource": "VictoriaMetrics",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(spam2000_latency_seconds_bucket[5m])) by (le))",
            "legendFormat": "Latency (s)",
            "refId": "D"
          }
        ],
        "id": 4,
        "gridPos": {
          "h": 6,
          "w": 24,
          "x": 0,
          "y": 6
        }
      },
      {
        "type": "graph",
        "title": "CPU Usage",
        "datasource": "VictoriaMetrics",
        "targets": [
          {
            "expr": "rate(container_cpu_usage_seconds_total{container=\"spam2000\"}[1m]) * 100",
            "legendFormat": "CPU Usage (%)",
            "refId": "E"
          }
        ],
        "id": 5,
        "gridPos": {
          "h": 6,
          "w": 12,
          "x": 0,
          "y": 12
        }
      },
      {
        "type": "graph",
        "title": "Memory Usage",
        "datasource": "VictoriaMetrics",
        "targets": [
          {
            "expr": "container_memory_usage_bytes{container=\"spam2000\"}",
            "legendFormat": "Memory Usage (bytes)",
            "refId": "F"
          }
        ],
        "id": 6,
        "gridPos": {
          "h": 6,
          "w": 12,
          "x": 12,
          "y": 12
        }
      },
      {
        "type": "graph",
        "title": "Active Users",
        "datasource": "VictoriaMetrics",
        "targets": [
          {
            "expr": "spam2000_active_users",
            "legendFormat": "Active Users",
            "refId": "G"
          }
        ],
        "id": 7,
        "gridPos": {
          "h": 6,
          "w": 24,
          "x": 0,
          "y": 18
        }
      },
      {
        "type": "graph",
        "title": "Database Query Duration",
        "datasource": "VictoriaMetrics",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(spam2000_db_query_duration_seconds_bucket[5m])) by (le))",
            "legendFormat": "Query Duration (s)",
            "refId": "H"
          }
        ],
        "id": 8,
        "gridPos": {
          "h": 6,
          "w": 24,
          "x": 0,
          "y": 24
        }
      },
      {
        "type": "graph",
        "title": "HTTP Status Codes",
        "datasource": "VictoriaMetrics",
        "targets": [
          {
            "expr": "sum(rate(spam2000_http_requests_total{code=~\"[2-5][0-9][0-9]\"}[1m])) by (code)",
            "legendFormat": "HTTP {{ code }}",
            "refId": "I"
          }
        ],
        "id": 9,
        "gridPos": {
          "h": 6,
          "w": 24,
          "x": 0,
          "y": 30
        }
      }
    ],
    "schemaVersion": 27,
    "style": "dark",
    "tags": ["spam2000", "application"],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "title": "Spam2000 Application Monitoring",
    "uid": "spam2000-dashboard",
    "version": 2
  }  